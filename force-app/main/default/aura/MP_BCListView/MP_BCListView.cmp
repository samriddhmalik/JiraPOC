                  <td
                    scope="row"
                    class="slds-text-align_center slds-cell_action-mode"
                  >
                    {!ArrFlight.departure_date_time}
                  </td>
                  <td
                    scope="row"
                    class="slds-text-align_center slds-cell_action-mode"
                  >
                    {!ArrFlight.Departure_Time}
