              </aura:iteration>
            </tbody>
          </table>

          <!--div class="" title="{!flight.Arrival_Airport_code}" style = "color:Black; font-size: 15px;">{!flight.Arrival_Airport_code}</div>-->
        </div>
        <!--Modal/Popup Box Footer Starts here-->
        <footer class="slds-modal__footer">
          <div class="slds-align_absolute-center">
            <lightning:button
              variant="brand"
              label="Close"
              title="Close"
              onclick="{! c.closeFlightManifest }"
            />
          </div>
        </footer>
      </div>
