            <aura:if isTrue="{!trip.Tripcase != null}">
              <div
                class=""
                title="{!trip.Tripcase}"
                style="color: Black; font-size: 15px"
              >
                <a href="{!trip.Tripcase}" target="_blank">Trip case 1</a>
              </div>
            </aura:if>

            <aura:if isTrue="{!trip.Tripcase1 != null}">
              <div
                class=""
                title="{!trip.Tripcase1}"
                style="color: Black; font-size: 15px"
              >
                <a href="{!trip.Tripcase1}" target="_blank">Trip case 2</a>
              </div>
            </aura:if>

            <aura:if isTrue="{!trip.Tripcase2 != null}">
              <div
                class=""
                title="{!trip.Tripcase2}"
                style="color: Black; font-size: 15px"
              >
                <a href="{!trip.Tripcase2}" target="_blank">Trip case 3</a>
              </div>
            </aura:if>
          </aura:iteration>
        </div>
        <!--Modal/Popup Box Footer Starts here-->
        <footer class="slds-modal__footer">
          <div class="slds-align_absolute-center">
            <lightning:button
              variant="brand"
              label="Close"
              title="Close"
              onclick="{! c.closeTripCase }"
            />
          </div>
        </footer>
      </div>
    </section>
    <div class="slds-backdrop slds-backdrop_open"></div>
  </aura:if>
 <aura:if isTrue="{!v.openFlightManifest}">
