     <aura:attribute type="Boolean" name="onStartDateSelectionSorting" default="false" />
     <aura:attribute name="isModalOpen" type="boolean" default="false"/>

  <aura:attribute
    name="isDepartureDateFetched"
    type="boolean"
    default="false"
  />
  <aura:attribute type="String" name="dealIDtoLWC" default="" />

  <aura:handler name="init" value="{!this}" action="{!c.doInit}" />

  <!--aura:handler event="force:refreshView" action="{!c.doInit}" />-->

  <!-- Neutral variant (default) -->

  <!--loading spinner start-->
  <aura:if isTrue="{!v.Spinner}">
    <div aura:id="dataspinnerId" class="slds-spinner_container">
      <div
        class="slds-spinner--brand slds-spinner slds-spinner--large slds-is-relative"
        role="alert"
      >
        <span class="slds-assistive-text">Loading...</span>
        <div class="slds-spinner__dot-a"></div>
        <div class="slds-spinner__dot-b"></div>
          <div class="custom-loading-text">
              <b>Please Wait While Loading...</b>
          </div>
      </div>
    </div>
  </aura:if>
    
    
  <!--loading spinner END-->

  <center>
    <h2 class="slds-page-header__title slds-truncate">BOOKING CONFIRMATION</h2>
  </center>
  <br />
  <div class="slds-align_absolute-left">
    <lightning:button
      variant="brand"
      label="Raise A Question"
      title="Case"
      onclick="{!c.NavigateToCase}"
    />
  </div>
  <br />

  <!--aura:if isTrue="{!v.filterFlag}">-->

  <div class="slds-box">
    <h3 class="$color-background-stencil-alt">Filters</h3>
    <br />
    <div class="slds-grid">
      <div class="slds-size_1-of-2">
        <lightning:select
          aura:id="dealPicklist"
          value="{!v.dealdata}"
          name="deals"
          label="Select Deal Name"
          required="true"
          onchange="{!c.onDealChange}"
        >
        <aura:if isTrue="{!v.dealPreviousName != null}">
            <aura:iteration items="{!v.dealList}" var="deal">
              <option value="{!deal.Id}">{!deal.Name}-{!deal.title__c}</option>
            </aura:iteration>
            <aura:set attribute="else">
              <!--<option value="">All Deals</option>-->
              <aura:iteration items="{!v.dealList}" var="deal">
               <option value="{!deal.Id}" selected = "{!deal.Id == v.dealList[1].Id}">
          
                     <aura:if isTrue="{!deal.Name == 'All Deals'}">
                         {!deal.Name}
                   
                     <aura:set attribute="else">
                         {!deal.Name}-{!deal.title}
                         </aura:set>
                    </aura:if>
               
                </option>
              </aura:iteration>
            </aura:set>
          </aura:if>
         </lightning:select>
      </div>
      <br />

      <div class="slds-size_1-of-2">
        <lightning:select
          aura:id="departurePicklist"
          value="{!v.deptdata}"
          name="departureDate"
          label="Select Departure Date"
          required="true"
          onchange="{!c.onDepartureDateChange}"
        >
          <!--<option value="">choose one...</option>
                    <aura:iteration items="{!v.depDateList}" var="dep">
                        <option value="{!dep.Id}" >{!dep.Name}</option>
                    </aura:iteration>-->
            <aura:if isTrue="{!v.depPreviousName != null}">
                <aura:iteration items="{!v.depDateList}" var="dep">
                    <option value="{!dep.Name}">{!dep.Name}</option>
                </aura:iteration>
                <aura:set attribute="else">
                    <option value="All Departure Date">All Departure Dates</option>
                    <aura:if isTrue="{!v.isDeptDateSelected}">
                        <aura:iteration items="{!v.depDateList}" var="dep">
                            <option value="{!dep}">{!dep}</option>
                        </aura:iteration>
                        <aura:set attribute="else">
                            <aura:iteration items="{!v.depDateListonDealChange}" var="dept">
                                <option value="{!dept.Name}">{!dept.Name}</option>
                            </aura:iteration>
                        </aura:set>
                    </aura:if>
            </aura:set>
          </aura:if>
        </lightning:select>
      </div>
    </div>

    <div class="slds-grid">
      <div class="slds-size_1-of-2">
        <lightning:select
          aura:id="startdatepicklist"
          name="startDate"
          label="Select Start Date"
          onchange="{!c.onStartDateChange}"
        >
          <aura:if isTrue="{!v.depPreviousName != null}">
            <aura:iteration items="{!v.startdateDataList}" var="sd">
              <option value="{!sd.Id}">{!sd}</option>
            </aura:iteration>
            <aura:set attribute="else">
              <option value="">All Start Dates</option>
              <aura:iteration items="{!v.startdateDataList}" var="sd">
                <option value="{!sd.Id}">{!sd}</option>
              </aura:iteration>
            </aura:set>
          </aura:if>
        </lightning:select>
      </div>
      <aura:if isTrue="{!v.isDealSelected}">
        <div
          class="slds-size_1-of-2 slds-align_absolute-center"
          style="font-size: 18px; font-weight: bold; padding-top: 25px"
        >
          <h2
            class="slds-page-header__title slds-truncate"
            style="color: #000000"
          >
            PDF Deal itinerary:&nbsp;
          </h2>
          <lightning:formattedUrl
            class="titleColor"
            value="{!v.dealPDFUrl}"
            label="{!v.dealIdTitle}"
          ></lightning:formattedUrl>
        </div>
      </aura:if>
    </div>
  </div>
  <!--/aura:if>-->
  <br />
  <br />

  <aura:if isTrue="{!v.isDepartureDateFetched}">
