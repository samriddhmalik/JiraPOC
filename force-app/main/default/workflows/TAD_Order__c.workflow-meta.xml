    <rules>
        <fullName>Amount Outstanding should be greater than zero%2E</fullName>
        <actions>
            <name>Amount_Outstanding</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>TAD_Order__c.ordexp_amount_outstanding__c</field>
            <operation>greaterThan</operation>
            <value>AUD 0</value>
        </criteriaItems>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Set Order SubStatus as Initial Payment Pending</fullName>
        <actions>
            <name>Set_Default_Sub_Status</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>TAD_Order__c.ordexp_master_status__c</field>
            <operation>equals</operation>
            <value>On Hold</value>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>TAD Order Payment Received Checkbox</fullName>
        <actions>
            <name>Payment_Received_Checkbox</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>TAD_Order__c.ordexp_total_payment_received__c</field>
            <operation>greaterThan</operation>
            <value>AUD 0</value>
