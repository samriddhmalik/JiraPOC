    <fieldUpdates>
        <fullName>Update_Credit_Utilized_On_OLI</fullName>
        <field>Credit_Utilized__c</field>
        <formula>IF(ordexp_TAD_Order__r.Total_Credit_Remaining__c  -  orderxp_total_amount__c  &gt;= 0, orderxp_total_amount__c , ordexp_TAD_Order__r.Total_Credit_Remaining__c)</formula>
        <name>Update Credit Utilized On OLI</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Formula</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <rules>
        <fullName>Populate Credit Utilized Field On OLI</fullName>
        <actions>
            <name>Update_Credit_Utilized_On_OLI</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 AND 2</booleanFilter>
        <criteriaItems>
            <field>TAD_Order__c.Total_Credit_Remaining__c</field>
            <operation>notEqual</operation>
            <value>AUD 0</value>
        </criteriaItems>
        <criteriaItems>
            <field>order_line_item__c.isSplit__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
    </rules>
</Workflow>
