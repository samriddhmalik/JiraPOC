<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_phone_number__c</fullName>
    <description>Points to mobile if there is a mobile if not then will pull regular phone field from account</description>
    <externalId>false</externalId>
    <formula>IF( 
    NOT(ISBLANK(Account__r.PersonContact.MobilePhone)),
    Account__r.PersonContact.MobilePhone,
    (Account__r.Phone)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Account phone number</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
