<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Min_Numbers_Email_Sent_no_changes</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
ISPICKVAL(Min_Numbers_Met_Send_Email__c ,&quot;Email Sent&quot;),
NOT ( $Permission.Min_Numbers_Email_Sent_no_changes )
)</errorConditionFormula>
    <errorMessage>No order changes allowed - the Min Numbers Met email has been sent. Please check with Teige first.</errorMessage>
</ValidationRule>
