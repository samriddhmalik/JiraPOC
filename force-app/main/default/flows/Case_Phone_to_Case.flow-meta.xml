    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
ISNEW() ,
OR(
ISPICKVAL([Case].SupportFor__c, &quot;Pre&quot;), 
ISPICKVAL([Case].SupportFor__c, &quot;Post&quot;), 
ISPICKVAL([Case].SupportFor__c, &quot;General&quot;), 
ISPICKVAL([Case].SupportFor__c, &quot;On Tour&quot;)
)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(
ISNEW() ,
OR(
ISPICKVAL({!myVariable_current.SupportFor__c}, &quot;Pre&quot;), 
ISPICKVAL({!myVariable_current.SupportFor__c}, &quot;Post&quot;), 
ISPICKVAL({!myVariable_current.SupportFor__c}, &quot;General&quot;), 
ISPICKVAL({!myVariable_current.SupportFor__c}, &quot;On Tour&quot;)
)
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
ISNEW() ,
ISPICKVAL([Case].Origin , &quot;Email&quot;) 
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_3</name>
        <dataType>Boolean</dataType>
        <expression>AND(
