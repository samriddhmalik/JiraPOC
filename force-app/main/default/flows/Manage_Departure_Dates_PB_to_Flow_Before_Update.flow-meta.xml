    <decisions>
        <name>Checking_for_HSS_Amount_AU_on_Departure_Date_if_its_changed</name>
        <label>Checking for HSS Amount AU on Departure Date if its changed</label>
        <locationX>732</locationX>
        <locationY>863</locationY>
        <defaultConnector>
            <targetReference>Assigning_Default_Value_for_HSS_AU_Amount</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>If_the_HSS_AU_Amount_is_updated_and_increased</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.HSS_amount__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assigning_value_into_HSSAU_Variable_as_True</targetReference>
            </connector>
            <label>If the HSS AU Amount is updated and increased</label>
        </rules>
        <rules>
            <name>If_the_HSS_AU_Amount_is_updated_and_decreased</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.HSS_amount__c</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assigning_value_into_HSSAU_Variable_as_False</targetReference>
            </connector>
            <label>If the HSS AU Amount is updated and decreased</label>
        </rules>
    </decisions>
